"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "modulo" table = "MODULO" catalog="TEST">
      
      <id name = "id">
      	 <column name = "id_modulo"/>
         <generator class="assigned"/>
      </id>
      
      <property name = "nombre" />
      <property name = "codigo" />
      
   </class>
</hibernate-mapping>