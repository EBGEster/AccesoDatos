<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "Alumno" table = "ALUMNO">
      
      <meta attribute = "class-description">
         This class contains the employee detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "nombre" column = "nombre_alumno" type = "string"/>
      <property name = "nacionalidad" column = "nacionalidad" type = "string"/>
      <property name = "edad" column = "edad" type = "int"/>
      <property name = "sexo" column = "sexo" type = "string"/>
      <property name = "list" column = "list" type = "modulo"/>
      
   </class>
</hibernate-mapping>